{% extends 'base.html.twig' %}

{% block title %}Joueur index{% endblock %}

{% block body %}
    <h1>Joueur index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id_joueur</th>
                <th>Nom_joueur</th>
                <th>Cin</th>
                <th>Url_photo</th>
                <th>Id_user</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for joueur in joueurs %}
            <tr>
                <td>{{ joueur.idJoueur }}</td>
                <td>{{ joueur.nomJoueur }}</td>
                <td>{{ joueur.cin }}</td>
                <td>{{ joueur.urlPhoto }}</td>
                <td>{{ joueur.idUser }}</td>
                <td>
                    <a href="{{ path('app_joueur_show', {'id_joueur': joueur.id_joueur}) }}">show</a>
                    <a href="{{ path('app_joueur_edit', {'id_joueur': joueur.id_joueur}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_joueur_new') }}">Create new</a>
{% endblock %}
