{% extends 'base.html.twig' %}

{% block title %}Equipe index{% endblock %}

{% block body %}
    <h1>Equipe index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id_equipe</th>
                <th>Nom_equipe</th>
                <th>Type_equipe</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for equipe in equipes %}
            <tr>
                <td>{{ equipe.idEquipe }}</td>
                <td>{{ equipe.nomEquipe }}</td>
                <td>{{ equipe.typeEquipe }}</td>
                <td>
                    <a href="{{ path('app_equipe_show', {'id_equipe': equipe.id_equipe}) }}">show</a>
                    <a href="{{ path('app_equipe_edit', {'id_equipe': equipe.id_equipe}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_equipe_new') }}">Create new</a>
{% endblock %}
